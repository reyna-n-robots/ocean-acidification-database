		<script>
		function ShowHideBibInfoTableRows( table, image )
		{
			var isIE7 = false;
			var imageDirectory = "/EOSWebOPAC/Images";
			var actionPerformed = "";
			for ( var index = 0; index < table.rows.length; index++ )
			{
				if (table.rows[index].getAttribute("data-alwaysvisible") == "false" )
				{
					if ( index == table.rows.length - 1 )
					{
						if (actionPerformed == "collapse")
						{
							table.rows[index].style.display = "table-row";
						}
						else
						{
							table.rows[index].style.display = "none";
						}
					}
					else
					{
						if ( table.rows[index].style.display == "table-row" )
						{
							table.rows[index].style.display = "none";
							if (actionPerformed == "") actionPerformed = "collapse";
						}
						else
						{
							table.rows[index].style.display = "table-row";
							if (actionPerformed == "") actionPerformed = "expand";
						}
					}
				}
			}
			if (actionPerformed != "")
			{
				image.setAttribute("src", imageDirectory + ((actionPerformed == "expand") ? "/arrow_colapse.png" : "/arrow_expand.png"));
				document.getElementById("BibDetailIsCollapsed").value = (actionPerformed == "expand") ? "false" : "true"; 
				if (actionPerformed == "expand") AdjustLayoutPanels(g_layoutCode);
			}
		}
		</script>
		
